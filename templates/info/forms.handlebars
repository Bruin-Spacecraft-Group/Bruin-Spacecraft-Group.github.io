<main class="animatedSlow fadeIn">
    <div id="forms-wrapper">
        <div class="project-header">
            <div class="parallax-image-header parallax-image"></div>
            <div class="project-header__title">
                <h1>Forms</h1>
            </div>
        </div>
        <div class="project-intro dark">
            <h1>For all your Bruin Space related forms!</h1>
        </div>
        <div class="parallax-image-header__follow parallax-image"></div>
        <div class="form-cards__container">
            {{#each forms}}
            <a class="form-card" href="{{link}}">
                <img src="{{image}}">
                <div class="card-container">
                    <h3>{{title}}</h3>
                </div>
            </a>
            {{/each}}
        </div>
    </div>
</main>
